<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
    <children>
        <Button fx:id="login_login" layoutX="59.0" layoutY="128.0" onAction="#handleLoginAction" text="Login" />
        <Label id="login_username" fx:id="login_username" layoutX="25.0" layoutY="56.0" minHeight="16" minWidth="69" text="Username:" />
      <Label id="login_password" fx:id="login_password" layoutX="25.0" layoutY="92.0" text="Password:" />
      <PasswordField fx:id="login_passwordfield" layoutX="126.0" layoutY="88.0" />
      <TextField fx:id="login_usernamefield" layoutX="126.0" layoutY="52.0" />
      <Button fx:id="login_register" layoutX="215.0" layoutY="128.0" mnemonicParsing="false" onAction="#handleRegisterAction" text="Register" />
      <Label fx:id="login_error" layoutX="60.0" layoutY="162.0" />
      <Label fx:id="login_regsuccess" layoutX="50.0" layoutY="21.0" />
    </children>
</AnchorPane>
